
<!--メッセージ一覧センターに寄せる-->
 <div class="col-sm-offset-2 col-sm-8">


 
 

<div class="panel panel-default">
  <div class="panel-heading">
    メッセージ一覧
  </div>

  <div class="panel-body">
    <% @messages.each do |message| %>
      <span class="label label-warning"><%= message.name %></span>
      <%= message.body %>
      <br>
    <% end %>
  </div>
</div>

<% #部分テンプレートを読み込んでいる %>
<%= render 'form' %>

 
 
